<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView
        android:id="@+id/iv_coupons_gift"
        style="@style/w_wrap_h_wrap"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="@dimen/expe_dialog_content_margin_top"
        android:src="@drawable/yhq_package" />

    <LinearLayout
        android:id="@+id/ll_coupons_gift_look"
        android:layout_width="@dimen/expe_btn_w"
        android:layout_height="@dimen/register_edit_h"
        android:layout_below="@id/iv_coupons_gift"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/margin_top_35"
        android:background="@drawable/button"
        android:gravity="center" >

        <TextView
            style="@style/text_style_nomal_28"
            android:text="@string/go_look"
            android:textColor="@color/app_white" />
    </LinearLayout>

</RelativeLayout>